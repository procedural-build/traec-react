{
  "name": "traec-react",
  "version": "0.1.58",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@azure/msal-browser": "^2.28.0",
    "@tinymce/tinymce-react": "^4.1.0",
    "axios": "^0.27.2",
    "d3": "^7.6.1",
    "d3-tip": "^0.9.1",
    "jwt-decode": "^3.1.2",
    "jwt-encode": "^1.0.1",
    "moment": "^2.29.4",
    "node": "^18.7.0",
    "nodejs": "^0.0.0",
    "react": "^18.2.0",
    "react-date-picker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-dropdown-select": "^4.9.0",
    "react-dropzone": "^14.2.2",
    "react-markdown": "^8.0.3",
    "react-octicon": "^3.0.1",
    "react-recaptcha": "^2.3.10",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.4.0",
    "react-style-proptype": "^3.2.2",
    "recharts": "^2.1.13",
    "sweetalert2": "^11.4.24",
    "traec": "^1.1.207"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^14.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --print-width 120"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/setupTests.js"
    ],
    "automock": false,
    "setupFiles": [
      "./test/setupJest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|sass)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^AppSrc(.*)$": "<rootDir>/src/$1"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "matchversion": "npm --no-git-tag-version --allow-same-version version $(npm view traec-react version)",
    "patchversion": "npm --no-git-tag-version version patch",
    "build": "npx babel src --out-dir ./dist && cp -v ./package.json ./dist/package.json && cd ./src && find ./ -type f ! -name \"*.js\" -exec cp -v --parents \\{\\} ../dist \\;",
    "pub": "npm run matchversion && npm run patchversion && npm run build && npm publish ./dist",
    "prepare": "husky install",
    "run-pretty-quick": "pretty-quick --staged --print-width 120",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "documentation": "npx babel src --out-dir ./dist && documentation serve --watch ../traec-react/dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/procedural-build/traec.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/procedural-build/traec/issues"
  },
  "homepage": "https://github.com/procedural-build/traec#readme"
}
